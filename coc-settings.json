{
	"rust-analyzer.cargo-watch.enable": true,
	"rust-analyzer.cargo-watch.allTargets": true,
	"rust-analyzer.cargo-watch.command": "clippy",
	"rust-analyzer.serverPath": "~/.cargo/bin/rust-analyzer",
	"python.linting.flake8Enabled": true,
	"python.linting.flake8Args": ["--max-line-length", "119"],
	"python.linting.flake8Path": "~/.local/bin/flake8",
	"python.linting.pylintEnabled": false
}
